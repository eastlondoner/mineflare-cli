{
  "name": "mineflare-cli",
  "version": "1.3.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/eastlondoner/mineflare-cli.git"
  },
  "type": "commonjs",
  "main": "src/server.js",
  "scripts": {
    "start": "bun src/server.js",
    "cli": "bun src/cli.js",
    "mineflare": "bun src/mineflare.js",
    "config": "bun src/config-cli.js",
    "test": "bun test",
    "test:unit": "bun test test/unit",
    "test:integration": "bun test test/integration",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:ci": "bun test --coverage --bail",
    "test:e2e": "bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e",
    "test:e2e:server": "bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e/server-lifecycle.e2e.test.js",
    "test:e2e:bot": "bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e/bot-connection.e2e.test.js",
    "test:e2e:api": "bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e/api.e2e.test.js",
    "test:e2e:stress": "E2E_TIMEOUT=60000 bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e/stress.e2e.test.js",
    "test:e2e:ci": "E2E_CLEANUP=true E2E_VERBOSE=false bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e",
    "test:e2e:verbose": "E2E_VERBOSE=true bun test --preload ./test/e2e/utils/e2e-setup.js test/e2e",
    "test:e2e:docker": "docker-compose -f test/e2e/docker-compose.yml up -d && bun test:e2e && docker-compose -f test/e2e/docker-compose.yml down",
    "build": "bun scripts/build-with-version.js --compile --minify --sourcemap ./src/mineflare.js --outfile mineflare",
    "build:linux": "bun scripts/build-with-version.js --compile --minify --sourcemap --target=bun-linux-x64 ./src/mineflare.js --outfile mineflare-linux",
    "build:linux-arm64": "bun scripts/build-with-version.js --compile --minify --sourcemap --target=bun-linux-arm64 ./src/mineflare.js --outfile mineflare-linux-arm64",
    "build:mac": "bun scripts/build-with-version.js --compile --minify --sourcemap --target=bun-darwin-arm64 ./src/mineflare.js --outfile mineflare-mac",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "keywords": ["minecraft", "bot", "mineflare", "ai", "mineflayer"],
  "author": "",
  "license": "ISC",
  "description": "AI-controlled Minecraft bot with HTTP API and CLI interface",
  "dependencies": {
    "axios": "^1.12.2",
    "canvas": "^3.2.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "mineflayer": "^4.33.0",
    "prismarine-viewer": "^1.33.0",
    "supertest": "^7.1.4"
  }
}
