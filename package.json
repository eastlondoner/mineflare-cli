{
  "name": "mineflare-cli",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eastlondoner/mineflare-cli.git"
  },
  "type": "commonjs",
  "main": "src/server.js",
  "scripts": {
    "start": "bun src/server.js",
    "cli": "bun src/cli.js",
    "mineflayer": "bun src/mineflayer.js",
    "config": "bun src/config-cli.js",
    "build": "bun build --compile --minify --sourcemap ./src/mineflayer.js --outfile mineflayer",
    "build:linux": "bun build --compile --minify --sourcemap --target=bun-linux-x64 ./src/mineflayer.js --outfile mineflayer-linux",
    "build:windows": "bun build --compile --minify --sourcemap --target=bun-windows-x64 ./src/mineflayer.js --outfile mineflayer.exe",
    "build:mac": "bun build --compile --minify --sourcemap --target=bun-darwin-arm64 ./src/mineflayer.js --outfile mineflayer-mac",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "keywords": ["minecraft", "bot", "mineflayer", "ai"],
  "author": "",
  "license": "ISC",
  "description": "AI-controlled Minecraft bot with HTTP API and CLI interface",
  "dependencies": {
    "axios": "^1.12.2",
    "canvas": "^3.2.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "mineflayer": "^4.33.0",
    "prismarine-viewer": "^1.33.0"
  }
}
