modules = ["bun-1.2"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["libuuid", "cairo", "pango", "libpng", "libjpeg", "jre", "jdk", "gh", "nano"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Bot Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Minecraft Server"

[[workflows.workflow]]
name = "Bot Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bun src/server.js"
waitForPort = 3000

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Minecraft Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd minecraft-server && java -Xmx1024M -Xms1024M -jar paper-1.21.8.jar nogui"
waitForPort = 8099

